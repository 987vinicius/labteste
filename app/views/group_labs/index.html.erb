<div class="box box-danger">
  <div class="box-header with-border inside">
    <div class="card-header with-border">
      <h3 class="card-title">
        <strong>
          <i class="fa fa-list"></i>
          Exames
        </strong>
      </h3>
    </div>

      <div class="card-body">
        <div class="row">
          <div class="col-md-12">
            <table class="table table-striped bootstrap-table-paginated">
              <thead>
                <tr>
                  <th class='text-center'>Nome</th>
                  <th class='text-center'>Tipo de exame</th>
                  <th class='text-center'>Status</th>
                  <th class='text-center'>Laboratório</th>
                  <th class='text-center'>Ações</th>
                </tr>
              </thead>
            <tbody>
              <% @laboratorios.each do |b| %>
                <tr>
                  <td><%= %></td>
                  <td><%= %></td>
                  <td><%= %></td>
                  <td><%= %></td>
                  <td class='text-center'>
                    <%= render 'shared/action_links', object: b %>
                  </td>
                </tr>
              <% end %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
    <div class="card-footer">
      <%= link_to 'Criar', new_group_lab_path, class: "btn btn-danger" %>
    </div>
  </div>
</div>
