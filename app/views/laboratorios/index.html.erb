<p id="notice"><%= notice %></p>

<h1>Laboratorios</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Cnpj</th>
      <th>Rua</th>
      <th>Bairro</th>
      <th>Logradouro</th>
      <th>Numero</th>
      <th>Complemento</th>
      <th>Cep</th>
      <th>Cidade</th>
      <th>Estado</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @laboratorios.each do |laboratorio| %>
      <tr>
        <td><%= laboratorio.nome %></td>
        <td><%= laboratorio.cnpj %></td>
        <td><%= laboratorio.rua %></td>
        <td><%= laboratorio.bairro %></td>
        <td><%= laboratorio.logradouro %></td>
        <td><%= laboratorio.numero %></td>
        <td><%= laboratorio.complemento %></td>
        <td><%= laboratorio.cep %></td>
        <td><%= laboratorio.cidade %></td>
        <td><%= laboratorio.estado %></td>
        <td><%= laboratorio.status %></td>
        <td><%= link_to 'Show', laboratorio %></td>
        <td><%= link_to 'Edit', edit_laboratorio_path(laboratorio) %></td>
        <td><%= link_to 'Destroy', laboratorio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Laboratorio', new_laboratorio_path %>
